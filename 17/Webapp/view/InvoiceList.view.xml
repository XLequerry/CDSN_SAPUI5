<mvc:View
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <List
            headText="{i18n>invoiceListTitle}"
            class="sapUiResponsiveMargin"
            width="auto"
            items="{invoice>/Invoices}">
        <items>
            <ObjectListItem
                    title="{invoice>Quantity} X {invoice>ProductName}"/>
        </items>
    </List>
</mvc:View>